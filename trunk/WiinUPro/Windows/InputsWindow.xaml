<Window x:Class="WiinUPro.InputsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WiinUPro"
        mc:Ignorable="d"
        Title="InputsWindow" Height="371.2" Width="580.8">
    <Grid>
        <TabControl x:Name="tabControl" Margin="0,0,0.4,31.6">
            <TabItem Header="Keyboard">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Mouse">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Xbox 360">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Joystick">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <Button x:Name="acceptBtn" Content="Accept" HorizontalAlignment="Left" Margin="397,317,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="cancelBtn" Content="Cancel" HorizontalAlignment="Left" Margin="484,317,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
